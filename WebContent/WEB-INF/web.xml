<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
         	<servlet>
			<servlet-name>LogiServlet</servlet-name>
			<servlet-class>com.customer1.LogiServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>LogiServlet</servlet-name>
			<url-pattern>/log</url-pattern>
		</servlet-mapping>
		
		
		<servlet>
		<servlet-name>PatientRegister</servlet-name>
		<servlet-class>com.customer1.patientRegisterServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
			<servlet-name>PatientRegister</servlet-name>
			<url-pattern>/insert</url-pattern>
		</servlet-mapping>
		
			
		<servlet>
		<servlet-name>updateProfile</servlet-name>
		<servlet-class>com.customer1.updateProfileServlet</servlet-class>
		</servlet>
			
		<servlet-mapping>
			<servlet-name>updateProfile</servlet-name>
			<url-pattern>/update</url-pattern>
		</servlet-mapping>
		
		
		<servlet>
		<servlet-name>delete</servlet-name>
		<servlet-class>com.customer1.deleteServlet</servlet-class>
		</servlet>
			
		<servlet-mapping>
			<servlet-name>delete</servlet-name>
			<url-pattern>/delete</url-pattern>
		</servlet-mapping>
		
		      



</web-app>  